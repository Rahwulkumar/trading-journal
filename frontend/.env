# Frontend Environment File (.env)
# Place this in your React app root directory

REACT_APP_TRADEMADE_API_KEY=your_actual_api_key_here
REACT_APP_BACKEND_URL=http://localhost:800
REACT_APP_WS_URL=ws://localhost:8000/ws
REACT_APP_API_BASE_URL=http://localhost:8000

VITE_APP_TRADEMADE_API_KEY=your_actual_api_key_here
VITE_APP_API_BASE_URL=http://localhost:8000

# Backend Environment File (.env)
# Place this in your Python backend directory (same folder as main.py)

TRADEMADE_API_KEY=your_trademade_api_key_here
TRADEMADE_API_BASE_URL=https://api.trademade.com/v1
TRADEMADE_WS_URL=wss://ws.trademade.com/v1

# Google Drive Configuration (existing)
GOOGLE_DRIVE_CREDENTIALS_PATH=./service-account-key.json
GOOGLE_DRIVE_FOLDER_ID=your_folder_id_here

# Database Configuration
DATABASE_URL=sqlite:///./trading_journal.db

# Development Settings
DEBUG=True
CORS_ORIGINS=["http://localhost:3000", "http://localhost:5173"]